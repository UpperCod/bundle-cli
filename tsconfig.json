  "compilerOptions": {
    "moduleResolution":"Node",
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "ESnext",
    "allowSyntheticDefaultImports":true,
    "esModuleInterop":true,
    "lib": [
      "es2019"
    ]
  }
}